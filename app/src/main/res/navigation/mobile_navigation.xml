<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.covid_19info.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_global"
        android:name="com.example.covid_19info.ui.global.GlobalFragment"
        android:label="@string/title_global"
        tools:layout="@layout/fragment_global" />
    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.covid_19info.ui.setting.SettingFragment"
        android:label="@string/title_setting" />

</navigation>
